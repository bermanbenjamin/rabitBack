{
  "name": "rabit-graphql-nestjs-prisma",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "prismix": "npx prismix && prisma format",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "vercel-build": "prisma generate && npm run migrate:deploy && next build",
    "prisma:generate:dataproxy": "prisma generate PRISMA_CLIENT_ENGINE_TYPE=\"dataproxy\"",
    "prisma:generate:library": "prisma generate PRISMA_CLIENT_ENGINE_TYPE=\"library\"",
    "migrate:deploy": "prisma migrate deploy DATABASE_URL=\"$MIGRATE_DATABASE_URL\"",
    "migrate:dev": "prisma migrate dev DATABASE_URL=\"$MIGRATE_DATABASE_URL\"",
    "deploy": "npm ci && npm run build && npm run start:prod",
    "rimraf": "./node_modules/rimraf/bin.js"
  },
  "dependencies": {
    "@nestjs-steroids/environment": "^1.0.2",
    "@nestjs/apollo": "^10.1.4",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/graphql": "^10.1.4",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.6",
    "@paljs/plugins": "^4.1.0",
    "@prisma/client": "^4.5.0",
    "apollo-server-core": "^3.10.3",
    "apollo-server-express": "^3.10.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv-flow": "^3.2.0",
    "firebase-admin": "^11.2.0",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.6",
    "jwks-rsa": "^2.1.5",
    "pactum": "^3.2.3",
    "passport": "^0.6.0",
    "passport-firebase-jwt": "^1.2.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7"
  },
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.6",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.5",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "prisma-nestjs-graphql": "^17.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!@generated/**/*",
      "!**/**/*.module.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
